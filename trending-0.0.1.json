{
	"article": "//article[@class='Box-row']",
	"full_name":"//h2[@class=\"h3 lh-condensed\"]/a/@href",
	"name":"//h2[@class=\"h3 lh-condensed\"]/a/text()",
	"desc_repo":"//p[@class=\"col-9 color-fg-muted my-1 pr-4\"]/text()",
	"language":"//div[@class=\"f6 color-fg-muted mt-2\"]/span[@class=\"d-inline-block ml-0 mr-3\"]/span[@itemprop=\"programmingLanguage\"]/text()",
	"forks_count":"//div[@class=\"f6 color-fg-muted mt-2\"]/a[2]",
